    "                            vehicle[x, 1] = vehicle[perm_pickup[i], 1]\n",
    "                            vehicle[x, 2] = vehicle[perm_pickup[i], 2]\n",
    "\n",
    "                    ##############################################################################################################################\n",
    "                    # MAC'S GHETTO MUTEX LOCK ####################################################################################################\n",
