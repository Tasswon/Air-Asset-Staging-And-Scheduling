   "metadata": {},
   "outputs": [],
   "source": [
    "START = time.time()\n",
    "\n",
    "vehicle = np.zeros(shape=(len(pickup),3), dtype='float32')\n",
    "\n",
    "for i in range(len(pickup)):\n",
    "    if pickup[i][2] == 6:\n",
    "        j = 0\n",
    "        while True:\n",
    "            col = rank_per_mission[:,i]\n",
    "            index = np.where(col == j)\n",
    "            if base[top_vals[index[0][0]]][2] != 5:\n",
